import pandas as pd
import numpy as np

"""Declaraçao de Funções"""

#Lê a aba de cada tabela

def Le_MatrizTRU(sCaminho, sTabelaRecursos, sPlanilha,  nLinhaInicial, nColunaInicial, nLinhas, nColunas):
  mSheet = pd.read_excel(sCaminho+sTabelaRecursos, sheet_name=sPlanilha, header=None, index_col=None)
  mMatriz = mSheet.values[nLinhaInicial:nLinhaInicial+nLinhas,nColunaInicial:nColunaInicial+nColunas]  
  return mMatriz
  
#Divide a tabela a precos correntes pela tabela a precos do ano anterior (constantes) resulta no deflator ano-ano por celula

def GeraDeflator(sCaminho, sTabelaCorrente, sTabelaAnterior,sPlanilha, nLinhaInicial,nColunaInicial,nLinhas,nColunas):
  mSheetCorrente  = pd.read_excel(sCaminho+sTabelaCorrente,sheet_name = sPlanilha, header=None, index_col=None)
  mSheetAnterior  = pd.read_excel(sCaminho+sTabelaAnterior,sheet_name = sPlanilha, header=None, index_col=None)
  mMatrizCorrente = mSheetCorrente.values[nLinhaInicial:nLinhaInicial+nLinhas,nColunaInicial:nColunaInicial+nColunas]
  mMatrizAnterior = mSheetAnterior.values[nLinhaInicial:nLinhaInicial+nLinhas,nColunaInicial:nColunaInicial+nColunas]
  mMatrizDividida = np.divide(mMatrizCorrente,mMatrizAnterior,  out=np.ones_like(mMatrizAnterior), where=mMatrizAnterior!=0)
  return mMatrizDividida
  
#Gera mudanças de preços relativos e mudanças de quantidades (de acordo com o método de deflacionamento de Reich)

def GeraDeflatorDeltaq(sCaminho, sTabelaCorrente, sTabelaAnterior,sPlanilha, nLinhaInicial,nColunaInicial,nLinhas,nColunas,deflator):
  mSheetCorrente  = pd.read_excel(sCaminho+sTabelaCorrente,sheet_name = sPlanilha, header=None, index_col=None)
  mSheetAnterior  = pd.read_excel(sCaminho+sTabelaAnterior,sheet_name = sPlanilha, header=None, index_col=None)
  mMatrizCorrente = mSheetCorrente.values[nLinhaInicial:nLinhaInicial+nLinhas,nColunaInicial:nColunaInicial+nColunas]
  mMatrizAnterior = mSheetAnterior.values[nLinhaInicial:nLinhaInicial+nLinhas,nColunaInicial:nColunaInicial+nColunas]
  mMnumerador     = mMatrizCorrente - mMatrizAnterior
  mMatrizDividida = mMnumerador*deflator
  return mMatrizDividida




