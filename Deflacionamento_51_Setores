  mCI011                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP01,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI021                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP02,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI031                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP03,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI041                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP04,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI051                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP05,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI061                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP06,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI071                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP07,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI081                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP08,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI091                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP09,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI101                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP10,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI111                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP11,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI121                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP12,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI131                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP13,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI141                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP14,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI151                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP15,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI161                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP16,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI171                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP17,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI181                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP18,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)
  mCI191                              = Le_MatrizTRU(sCaminho,sTab4+nAnoMIP19,sPlanilhaCI,nLinhaInicialCI,nColunaInicialCI,nLinhasCI,nColunasCI)  
  
  #calculando os deflatores encadeados do VA (pode ser utilizado o VBP no lugar - VA linha 10)

  mVBPDef0100                          = (np.sum(mProducao01) - np.sum(mCI01))/(np.sum(mProducao011) - np.sum(mCI011)) 
  mVBPDef0201                          = (np.sum(mProducao02) - np.sum(mCI02))/(np.sum(mProducao021) - np.sum(mCI021)) 
  mVBPDef0302                          = (np.sum(mProducao03) - np.sum(mCI03))/(np.sum(mProducao031) - np.sum(mCI031)) 
  mVBPDef0403                          = (np.sum(mProducao04) - np.sum(mCI04))/(np.sum(mProducao041) - np.sum(mCI041)) 
  mVBPDef0504                          = (np.sum(mProducao05) - np.sum(mCI05))/(np.sum(mProducao051) - np.sum(mCI051)) 
  mVBPDef0605                          = (np.sum(mProducao06) - np.sum(mCI06))/(np.sum(mProducao061) - np.sum(mCI061)) 
  mVBPDef0706                          = (np.sum(mProducao07) - np.sum(mCI07))/(np.sum(mProducao071) - np.sum(mCI071)) 
  mVBPDef0807                          = (np.sum(mProducao08) - np.sum(mCI08))/(np.sum(mProducao081) - np.sum(mCI081)) 
  mVBPDef0908                          = (np.sum(mProducao09) - np.sum(mCI09))/(np.sum(mProducao091) - np.sum(mCI091)) 
  mVBPDef1009                          = (np.sum(mProducao10) - np.sum(mCI10))/(np.sum(mProducao101) - np.sum(mCI101)) 
  mVBPDef1110                          = (np.sum(mProducao11) - np.sum(mCI11))/(np.sum(mProducao111) - np.sum(mCI111)) 
  mVBPDef1211                          = (np.sum(mProducao12) - np.sum(mCI12))/(np.sum(mProducao121) - np.sum(mCI121)) 
  mVBPDef1312                          = (np.sum(mProducao13) - np.sum(mCI13))/(np.sum(mProducao131) - np.sum(mCI131)) 
  mVBPDef1413                          = (np.sum(mProducao14) - np.sum(mCI14))/(np.sum(mProducao141) - np.sum(mCI141)) 
  mVBPDef1514                          = (np.sum(mProducao15) - np.sum(mCI15))/(np.sum(mProducao151) - np.sum(mCI151)) 
  mVBPDef1615                          = (np.sum(mProducao16) - np.sum(mCI16))/(np.sum(mProducao161) - np.sum(mCI161)) 
  mVBPDef1716                          = (np.sum(mProducao17) - np.sum(mCI17))/(np.sum(mProducao171) - np.sum(mCI171)) 
  mVBPDef1817                          = (np.sum(mProducao18) - np.sum(mCI18))/(np.sum(mProducao181) - np.sum(mCI181)) 
  mVBPDef1918                          = (np.sum(mProducao19) - np.sum(mCI19))/(np.sum(mProducao191) - np.sum(mCI191))  

  #calculando os precos relativos do VBP (VA por Residuo)
  
  mMPrecoRelativoVBP0019      =  mVBPDef0100 * mVBPDef0201 * mVBPDef0302 * mVBPDef0403 * mVBPDef0504 * mVBPDef0605 * mVBPDef0706 * mVBPDef0807 * mVBPDef0908 * mVBPDef1009 * mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP0119      =  mVBPDef0201 * mVBPDef0302 * mVBPDef0403 * mVBPDef0504 * mVBPDef0605 * mVBPDef0706 * mVBPDef0807 * mVBPDef0908 * mVBPDef1009 * mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP0219      =  mVBPDef0302 * mVBPDef0403 * mVBPDef0504 * mVBPDef0605 * mVBPDef0706 * mVBPDef0807 * mVBPDef0908 * mVBPDef1009 * mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP0319      =  mVBPDef0403 * mVBPDef0504 * mVBPDef0605 * mVBPDef0706 * mVBPDef0807 * mVBPDef0908 * mVBPDef1009 * mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP0419      =  mVBPDef0504 * mVBPDef0605 * mVBPDef0706 * mVBPDef0807 * mVBPDef0908 * mVBPDef1009 * mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP0519      =  mVBPDef0605 * mVBPDef0706 * mVBPDef0807 * mVBPDef0908 * mVBPDef1009 * mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP0619      =  mVBPDef0706 * mVBPDef0807 * mVBPDef0908 * mVBPDef1009 * mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP0719      =  mVBPDef0807 * mVBPDef0908 * mVBPDef1009 * mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP0819      =  mVBPDef0908 * mVBPDef1009 * mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP0919      =  mVBPDef1009 * mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP1019      =  mVBPDef1110 * mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP1119      =  mVBPDef1211 * mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP1219      =  mVBPDef1312 * mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP1319      =  mVBPDef1413 * mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP1419      =  mVBPDef1514 * mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP1519      =  mVBPDef1615 * mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP1619      =  mVBPDef1716 * mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP1719      =  mVBPDef1817 * mVBPDef1918
  mMPrecoRelativoVBP1819      =  mVBPDef1918

  #Gerando os indices de Deflacionamento (base ano 2000 - perceba que o ano base - aquele que divie a todos - deve ficar na ultima linha
  
  mMPrecoRelativoVBP1219                = np.divide(mMPrecoRelativoVBP1219,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP1319                = np.divide(mMPrecoRelativoVBP1319,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP1419                = np.divide(mMPrecoRelativoVBP1419,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP1519                = np.divide(mMPrecoRelativoVBP1519,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP1619                = np.divide(mMPrecoRelativoVBP1619,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP1719                = np.divide(mMPrecoRelativoVBP1719,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP1819                = np.divide(mMPrecoRelativoVBP1819,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0) 
  mMPrecoRelativoVBP1919                = np.divide(1,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP0119                = np.divide(mMPrecoRelativoVBP0119,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP0219                = np.divide(mMPrecoRelativoVBP0219,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP0319                = np.divide(mMPrecoRelativoVBP0319,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP0419                = np.divide(mMPrecoRelativoVBP0419,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP0519                = np.divide(mMPrecoRelativoVBP0519,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP0619                = np.divide(mMPrecoRelativoVBP0619,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
  mMPrecoRelativoVBP0719                = np.divide(mMPrecoRelativoVBP0719,mMPrecoRelativoVBP0019,out =np.ones_like(mMPrecoRelativoVBP0019),where = mMPrecoRelativoVBP0019!=0)
